{
  "name" : "DL Entry+",
  "version" : "0.1",
  "manifest_version" : 3,
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "activeTab", "webNavigation", "scripting", "tabs"
  ],
  "content_scripts" : [
      {
          "matches" : ["https://playentry.org/*"],
          "js" : ["index.js"],
          "css": ["index.css"]
      }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://playentry.org/*"]
}